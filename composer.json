{
    "name": "toastnz/externallinks",
    "description": "Module which tracks external broken links in SilverStripe CMS page content field and HTMLText field in toast blocks",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "content blocks",
        "external broken links",
        "modular"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ricky Li",
            "homepage": "http://rickyli.co.nz/",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "silverstripe/externallinks": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeModule\\Skeleton\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
